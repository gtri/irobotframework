{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "preserveWatchOutput": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "target": "es2015",
    "types": [],
    "lib": ["dom", "es2015"],
    "jsx": "react"
  }
}
